{
    "name": "bluebaytravel/phrontmatter",
    "description": "Frontmatter parser for PHP.",
    "license": "MIT",
    "authors": [{
        "name": "James Brooks",
        "email": "james@bluebaytravel.co.uk"
    }],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.1.*|5.2.*",
        "illuminate/support": "5.1.*|5.2.*",
        "symfony/yaml": "~2.7|~3.0",
        "jamesmoss/toml": "^1.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "autoload": {
        "psr-4": {
            "BlueBayTravel\\Phrontmatter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BlueBayTravel\\Tests\\Phrontmatter\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
