{
    "name": "bluebaytravel/phrontmatter",
    "description": "Front Matter parser for PHP, with support for YAML, JSON and TOML.",
    "license": "MIT",
    "authors": [{
        "name": "James Brooks",
        "email": "james@bluebaytravel.co.uk"
    }],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*|5.4.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*",
        "jamesmoss/toml": "^1.1",
        "symfony/yaml": "~2.7|~3.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "autoload": {
        "psr-4": {
            "BlueBayTravel\\Phrontmatter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BlueBayTravel\\Tests\\Phrontmatter\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        },
        "laravel": {
            "providers": [
                "BlueBayTravel\\Phrontmatter\\PhrontmatterServiceProvider"
            ],
            "aliases": {
                "Phrontmatter": "BlueBayTravel\\Phrontmatter\\Facades\\Phrontmatter"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
